	tS, _ := t.Parse("{{.huh}} and {{.hu}}")
	h:=Koko{"huhu","huh"}
	// h:=map[string]string{"huh":"huhtest","hus":"hush"}
	w:=map[string]string{"hu":"hutest"}
	// huh := "this is huhu"
	// hu := "myhuhu"
	bW := bytes.NewBufferString("")
	tS.Execute(bW,h)
	fmt.Println(bW)
